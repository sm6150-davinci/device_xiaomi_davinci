on property:ro.boot.hwc=GLOBAL
    enable vendor.nfc_hal_service

on property:ro.boot.hwc=CN
    enable vendor.nfc_hal_service

service vendor.nfc_hal_service /vendor/bin/hw/android.hardware.nfc@1.2-service
    override
    class hal
    user nfc
    group nfc
    task_profiles ServiceCapacityLow
    disabled
